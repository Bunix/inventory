{
    "name": "stevebauman/inventory",
    "description": "Inventory management using Laravel 4",
    "keywords": ["laravel", "inventory", "inventory-management"],
    "authors": [
        {
            "name": "Steve Bauman",
            "email": "steven_bauman_7@hotmail.com"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "4.*",
        "stevebauman/core-helper": "1.0.*",
        "baum/baum": "~1.0"
    },
    "suggest": {
        "venturecraft/revisionable": "Allows the tracking of inventory stock location and category changes"
    },
    "autoload": {
        "classmap": [
            "src/migrations"
        ],
        "psr-0": {
            "Stevebauman\\Inventory\\": "src/"
        }
    },
    "minimum-stability": "stable"
}
